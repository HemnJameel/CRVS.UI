@model IEnumerable<CRVS.Core.Models.Governorite>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GovernoriteName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.GovernoriteName)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.GovernoriteId">Edit</a>
                    <a asp-action="Details" asp-route-id="@item.GovernoriteId">Details</a>
                    <a asp-action="Delete" asp-route-id="@item.GovernoriteId">Delete</a>
                    <a asp-action="History" class="btn btn-primary" asp-route-id="@item.GovernoriteId">History</a>
                </td>
            </tr>
        }
    </tbody>
</table>


