@model IEnumerable<CRVS.Core.Models.Doh>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DohName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GovernoriteId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DohName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GovernoriteId)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.DohId">Edit</a>
                    <a asp-action="Details" asp-route-id="@item.DohId">Details</a>
                    <a asp-action="Delete" asp-route-id="@item.DohId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>


